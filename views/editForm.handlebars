<div class="editPage" >
	<form id="editPageInput">
		<div id="titleInput">
			<label for="pageTitle">Title of Page</label>
		    <input id="pageTitle" type="text" value={{title}}> 
	    </div>   

	    <div id="contentInput">
			<label for="pageContent">Content of Page</label>
		    <input id="pageContent" type="text" value={{content}}>  
		</div>


	    <input type="submit" id="submit" value="Submit" name="{{id}}"/>

	</form>
</div>